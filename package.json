{
  "name": "amazon-connect-chatjs",
  "version": "2.2.4",
  "main": "dist/amazon-connect-chat.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "directories": {
    "lib": "./dist"
  },
  "files": [
    "dist/",
    "CHANGELOG.md"
  ],
  "description": "Provides chat support to AmazonConnect customers",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "release": "tsc && npm run lint && jest && webpack --mode=production",
    "devo": "jest && webpack --mode=development",
    "watch": "webpack --watch",
    "dev": "webpack --mode=development && webpack --watch",
    "server": "webpack-dev-server --hot",
    "clean": "rm -rf build/ node_modules build",
    "release-watch": "npm run release && npm run watch",
    "posttest": "generate-coverage-data",
    "lint": "eslint --ext .js ./src",
    "lint:fix": "eslint --fix 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amazon-connect/amazon-connect-chatjs.git"
  },
  "keywords": [
    "amazon",
    "connect",
    "streamJs",
    "chatjs"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./test/jestSetup.js"
    ],
    "transform": {
      ".(js|jsx)$": "babel-jest"
    },
    "testRegex": "/src/.*\\.spec\\.js$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "json",
      "html",
      "cobertura",
      "lcov",
      "text"
    ]
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.15.4",
    "babel-loader": "^8.2.4",
    "eslint": "^8.9.0",
    "jest": "^27.5.1",
    "regenerator-runtime": "^0.13.9",
    "typescript": "^4.4.2",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.8.1",
    "@babel/eslint-parser": "^7.18.9",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-jest": "^26.6.0"
  },
  "dependencies": {
    "sprintf-js": "^1.1.2",
    "detect-browser": "5.3.0"
  }
}
