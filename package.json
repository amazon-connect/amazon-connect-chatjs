{
  "name": "amazonconnectchatjs",
  "version": "1.0.0",
  "description": "Provides chat support to AmazonConnect customers",
  "scripts": {
    "test": "jest",
    "release": "jest && webpack --mode=production",
    "watch": "webpack --watch",
    "dev": "webpack --mode=development && webpack --watch",
    "server": "webpack-dev-server",
    "clean": "rm -rf build/ node_modules build"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git.amazon.com:2222/pkg/AmazonConnectChatJS"
  },
  "keywords": [
    "amazon",
    "connect",
    "streamJs",
    "chatjs"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/test/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "author": "amazon",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@types/jest": "^23.3.9",
    "@types/webpack": "^4.4.19",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.9.0",
    "eslint-loader": "^2.1.1",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
