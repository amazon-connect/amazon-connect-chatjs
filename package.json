{
  "name": "amazon-connect-chatjs",
  "version": "1.4.0",
  "main": "dist/amazon-connect-chat.js",
  "types": "src/index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "directories": {
    "lib": "./dist"
  },
  "files": [
    "dist/",
    "src/index.d.ts"
  ],
  "description": "Provides chat support to AmazonConnect customers",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "release": "tsc && jest && webpack --mode=production",
    "devo": "jest && webpack --mode=development",
    "watch": "webpack --watch",
    "dev": "webpack --mode=development && webpack --watch",
    "server": "webpack-dev-server --hot",
    "clean": "rm -rf build/ node_modules build",
    "release-watch": "npm run release && npm run watch",
    "lint": "eslint --ext .js,.ts,.tsx ./",
    "lint:fix": "eslint --fix --ext .js,.jsx ./",
    "prettier": "prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "precommit": "pretty-quick --staged && npm test",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amazon-connect/amazon-connect-chatjs.git"
  },
  "keywords": [
    "amazon",
    "connect",
    "streamJs",
    "chatjs"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./test/jestSetup.js"
    ],
    "transform": {
      ".(js|jsx)$": "babel-jest"
    },
    "testRegex": "/src/.*\\.spec\\.js$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "json",
      "html",
      "cobertura",
      "lcov",
      "text"
    ]
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.15.4",
    "babel-loader": "^8.2.4",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "regenerator-runtime": "^0.13.9",
    "typescript": "^4.4.2",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "detect-browser": "5.3.0",
    "sprintf-js": "^1.1.2"
  }
}
