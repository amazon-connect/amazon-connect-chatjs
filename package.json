{
  "name": "@amzn/amazon-connect-chatjs",
  "version": "1.0.0",
  "main": "dist/amazon-connect-chat.js",
  "description": "Provides chat support to AmazonConnect customers",
  "scripts": {
    "test": "jest",
    "release": "jest && webpack --mode=production",
    "devo": "jest && webpack --mode=development",
    "watch": "webpack --watch",
    "dev": "webpack --mode=development && webpack --watch",
    "server": "webpack-dev-server --hot --inline",
    "clean": "rm -rf build/ node_modules build"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git.amazon.com:2222/pkg/AmazonConnectChatJS"
  },
  "keywords": [
    "amazon",
    "connect",
    "streamJs",
    "chatjs"
  ],
  "jest": {
    "transform": {
      ".(js|jsx)$": "babel-jest"
    },
    "testRegex": "/src/.*\\.spec\\.js$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "author": "amazon",
  "license": "ISC",
  "npm-pretty-much": {
    "runRelease": "always"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.9.0",
    "eslint-loader": "^2.1.1",
    "jest": "^23.6.0",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "sprintf-js": "^1.1.2"
  }
}
